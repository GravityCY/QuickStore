{
  "schemaVersion": 1,
  "id": "${mod_id}",
  "name": "${mod_name}",
  "version": "${mod_version}",
  "description": "${mod_description}",
  "authors": [
    "${mod_author}"
  ],
  "contact": {
    "homepage": "${mod_sources}",
    "sources": "${mod_sources}",
    "issues": "${mod_sources}"
  },
  "license": "${mod_license}",
  "icon": "assets/itemio/icon.png",
  "environment": "client",
  "entrypoints": {
    "client": [
      "me.gravityio.itemio.ItemIO"
    ],
    "modmenu": [
      "me.gravityio.itemio.compat.ModMenuApiImpl"
    ]
  },
  "mixins": [
    "itemio.mixins.json",
    "compat.mixins.json"
  ],
  "custom": {
    "modmenu": {
      "links": {
        "modmenu.discord": "https://discord.gg/Ef4Z22gnqj"
      }
    }
  },
  "depends": {
    "fabricloader": "*",
    "fabric-api": "*",
    "yet_another_config_lib_v3": "*",
    "minecraft": ">=${minecraft_version}",
    "java": ">=17"
  }
}